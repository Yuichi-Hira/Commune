 <div class="contents row">
  <%= form_for (@content) do |f| %>
    <h3>
      投稿する
    </h3>
    <%= f.text_field :title %>
    <div class="form-group">
    <label for="exampleInputFile">画像を選択してください</label>
    <div class="field">
    <%= f.file_field :image %>
    <form name="selbox">
<p>カテゴリーを選択してください</p>
<select name="category_id" >
<option value="">*カテゴリー選択</option>
<option value="">Studio</option>
<option value="">Creater</option>
<option value="">Event&Workshop</option>
<option value="">Item</option>
</select>

  </div>
  <p>テキストを入力してください</p>
    <%= f.text_field :body %>
    <%= f.submit "SENT" %>
  <% end %>
</div>
