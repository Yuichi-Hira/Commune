 <div class="contents row">
  <%= form_for (@content) do |f| %>
    <h3>
      投稿する
    </h3>
    <%= f.text_field :title %>
    <div class="form-group">
    <label for="exampleInputFile">画像を選択してください</label>
    <div class="field">
    <%= f.file_field :image %>
    <form name="selbox">
<p>カテゴリーを選択してください</p>
<select class="form-control select-parent" name="category[category_id]" id="category_id">
  <option value=""></option>
  <option data-children-path="/categories/1/" value="1">Studio</option>
  <option data-children-path="/categories/2/" value="2">Creater</option>
  <option data-children-path="/categories/3/" value="3">Event&Workshop</option>
  <option data-children-path="/categories/4/" value="4">Item</option>
</select>

  </div>
  <p>テキストを入力してください</p>
    <%= f.text_field :body %>
    <%= f.submit "SENT" %>
  <% end %>
</div>
