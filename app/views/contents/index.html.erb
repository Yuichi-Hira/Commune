    <div class="row">
    <% @contents.each do |content| %>
      <div class="col-sm-6 portfolio-item"> <a href="work-details.html" class="portfolio-link">
        <div class="caption">
          <div class="caption-content">
            <h3><%= content.title %></h3>
            <h4><%= content.contributor %></h4>
          </div>
        </div>
        <img src="<%= asset_path("#{content.image_url}") %>"  class="img-responsive" alt=""> </a>
        <% end %>
        <%= paginate(@contents) %>
